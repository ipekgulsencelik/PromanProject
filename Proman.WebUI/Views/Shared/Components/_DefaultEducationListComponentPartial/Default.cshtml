@model List<ResultEducationDTO>

<div id="tab-2" class="tab-pane fade show p-0">
    <div class="row gy-5 gx-4">
        <div id="tab-2" class="tab-pane fade show p-0">
            <div class="row gy-5 gx-4">
                <div class="col-sm-6">
                    <h5>UI Design Course</h5>
                    <hr class="text-primary my-2">
                    <p class="text-primary mb-1">2000 - 2045</p>
                    <h6 class="mb-0">Cambridge University</h6>
                </div>
                <div class="col-sm-6">
                    <h5>IOS Development</h5>
                    <hr class="text-primary my-2">
                    <p class="text-primary mb-1">2000 - 2045</p>
                    <h6 class="mb-0">Cambridge University</h6>
                </div>
                <div class="col-sm-6">
                    <h5>Web Design</h5>
                    <hr class="text-primary my-2">
                    <p class="text-primary mb-1">2000 - 2045</p>
                    <h6 class="mb-0">Cambridge University</h6>
                </div>
                <div class="col-sm-6">
                    <h5>Apps Design</h5>
                    <hr class="text-primary my-2">
                    <p class="text-primary mb-1">2000 - 2045</p>
                    <h6 class="mb-0">Cambridge University</h6>
                </div>
            </div>
        </div>
        @foreach (var item in Model)
        {
            <div class="col-sm-6">
                <h5>@item.EducationName</h5>
                <hr class="text-primary my-2">
                <p class="text-primary mb-1">@item.StartedAt.Year - @item.FinishedAt.Year</p>
                <h6 class="mb-0">@item.SchoolName</h6>
            </div>
        }
    </div>
</div>